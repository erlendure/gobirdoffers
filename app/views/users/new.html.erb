<h1>Sign up</h1>
<div class="row">
  <div class="span6 offset3">
  	<p><%= link_to "All Users", users_path %></p>
    <%= form_for(@user) do |f| %>
      <%= render 'shared/error_messages', object: f.object %>
      <%= render 'fields', f: f %>

      <%= f.submit "Create my account", class: "btn btn-large btn-primary" %>
    <% end %>
  </div>
</div>