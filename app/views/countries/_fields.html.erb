<div class="row">
  <div class="span6 offset1">
    <%= form_for(@country) do |f| %>
      <%= render 'shared/error_messages', object: f.object %>
	  
	  <%= f.label :country_name %>
      <%= f.text_field :country_name %>

      <%= f.label :default_locale_id %>
      <%= f.number_field :default_locale_id %>

	  </br></br>
			
      <%= f.submit btn_txt, class: "btn btn-primary" %>
    <% end %>
  </div>
</div>
