<tr>
  <td><%= link_to user.name, user %></td>
  <td><%= user.username %></td>
  <td><%= user.email %></td>  
  <td><%= link_to '', edit_user_path(user) , class: "icon-edit" %></td>
  <td><% if current_user.admin? && !current_user?(user) %> 
  	  <%= link_to '', user, method: :delete, confirm: "You sure?", class: "icon-remove" %>
  <% end %></td>
</tr>